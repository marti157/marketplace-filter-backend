{
  "env": {
    "es2021": true,
    "node": true
  },
  "plugins": [
    "prettier"
  ],
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "consistent-return": 0,
    "no-return-await": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-underscore-dangle": [2, { "allowAfterThis": true }],
    "spaced-comment": 0
    
  }
}
